{
  "name": "ember-cli-proxy-fixtures",
  "version": "0.0.3",
  "description": "Capture Ember CLI proxy server responses for faster playback on the second run.",
  "directories": {
    "doc": "doc",
    "test": "test"
  },
  "scripts": {
    "test": "mocha tests/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dockyard/ember-cli-proxy-fixtures"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "keywords": [
    "ember-addon"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "after": "ember-cli-qunit",
    "before": "proxy-server-middleware"
  },
  "author": {
    "name": "Brian Cardarella"
  },
  "license": "MIT",
  "dependencies": {
    "mkdirp": "0.5.0",
    "ember-cli-pretender": "0.2.3",
    "broccoli-json-concat": "0.0.4",
    "broccoli-file-mover": "0.4.0",
    "broccoli-static-compiler": "^0.1.4",
    "broccoli-concat": "0.0.11",
    "broccoli-merge-trees": "0.1.4",
    "broccoli-file-mover": "0.3.6"
  },
  "devDependencies": {
    "assert": "1.1.2",
    "mocha": "1.21.4",
    "sinon": "1.11.1"
  }
}
